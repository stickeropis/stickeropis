{
  "scripts": {
    "dev": "npm run dev:unix",
    "dev:unix": "NODE_PATH=$(pwd) nodemon index.js",
    "dev:windows": "set NODE_ENV=%cd% && nodemon index.js",
    "build": "next build",
    "start": "NODE_PATH=$(pwd) NODE_ENV=production node index.js",
    "lint": "npm-run-all lint:*",
    "lint:css": "stylelint ./**/*.scss",
    "lint:js": "eslint --ext .js,.jsx ./",
    "lint-fix": "npm-run-all lint-fix:*",
    "lint-fix:css": "stylelint --fix ./**/*.scss",
    "lint-fix:js": "eslint --fix --ext .js,.jsx ./",
    "prepush": "npm run lint"
  },
  "dependencies": {
    "@material-ui/core": "3.9.2",
    "@zeit/next-sass": "1.0.1",
    "bem-cn": "3.0.1",
    "express": "4.16.4",
    "isomorphic-fetch": "2.2.1",
    "jss": "9.8.7",
    "next": "7.0.2",
    "next-redux-wrapper": "3.0.0-alpha.0",
    "node-sass": "4.11.0",
    "npm-run-all": "4.1.5",
    "prop-types": "15.6.2",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-jss": "8.6.1",
    "react-redux": "6.0.0",
    "redux": "4.0.1",
    "redux-actions": "2.6.4",
    "redux-devtools-extension": "2.13.8"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.13.0",
    "eslint-config-iddqd": "1.2.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-react": "7.12.4",
    "husky": "1.3.1",
    "nodemon": "1.18.10",
    "stylelint": "9.10.1",
    "stylelint-config-iddqd": "1.1.0",
    "stylelint-order": "2.0.0",
    "stylelint-scss": "3.5.3"
  }
}
